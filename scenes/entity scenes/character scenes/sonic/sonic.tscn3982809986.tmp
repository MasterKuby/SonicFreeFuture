[sub_resource type="GDScript" id="GDScript_2rf57"]
script/source = "extends State

@onready var stateMachine = get_parent()
@onready var sonic = get_parent().get_parent()
@onready var sprite = $\"../../AnimatedSprite2D\"

func enter():
	if sonic.is_on_floor() and sonic.direction == 0:
		if sprite.animation != \"idle\" and !Input.is_action_pressed(\"w\") and sprite.animation != \"sliding\":
			sprite.animation = \"idle\"
			sprite.play()

func exit():
	pass
	
func update(delta):
	pass

func physicsUpdate(delta):
	if sonic.is_on_floor():
		sonic.dashed = false
	if sonic.is_on_floor() and sonic.direction == 0:
		if sprite.animation != \"idle\" and !Input.is_action_pressed(\"w\"):
			sprite.animation = \"idle\"
			sprite.play()
			sonic.dashed = false
	debugModeCheck()
	movingCheck()
	stoppingCheck()
	jumpCheck()
	fallingCheck()
	stoppingCheck()

func debugModeCheck(): # later check if this function needs to be in every script or if it can just be in sonic script then you call it from here and it still works
	if GV.debugMode == true and GV.activeCharacter == sonic.character:
		transitioned.emit(self, \"debug\")
func movingCheck():
	if Input.is_action_pressed(\"a\") or Input.is_action_pressed(\"s\") or Input.is_action_pressed(\"d\"):
		transitioned.emit(self, \"moving\")
	if sonic.direction or !sonic.is_on_floor():
		transitioned.emit(self, \"moving\")
func jumpCheck():
	if Input.is_action_pressed(\"w\") and sonic.is_on_floor() and GV.debugMode == false and GV.activeCharacter == sonic.character:
		transitioned.emit(self, \"jumping\")
func stoppingCheck():
	if abs(sonic.velocity.x) > 10*sonic.scale.x:
		transitioned.emit(self, \"stopping\")
func fallingCheck():
	if !sonic.is_on_floor() and !Input.is_action_pressed(\"w\") and sonic.velocity.y:
		transitioned.emit(self, \"falling\")
	
		
"

[sub_resource type="GDScript" id="GDScript_r8wl1"]
script/source = "extends State
# We cant directly transition to idle because we must go through stopping first
@onready var stateMachine = get_parent()
@onready var sonic = get_parent().get_parent()
@onready var sprite = $\"../../AnimatedSprite2D\"
@onready var dashingTimer = $\"../../Double Tap Dash Timer\"

func enter():
	pass

func exit():
	pass

func update(delta):
	pass

func physicsUpdate(delta):
	animationsCheck()
	applyGravity(delta)
	jumpingCheck()
	movement()
	debugModeCheck()
	fallingCheck()
	stoppingCheck()
	if sonic.is_on_floor():
		sonic.dashed = false

func debugModeCheck():
